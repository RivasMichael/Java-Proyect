	<queryString>
		<![CDATA[SELECT
     Mantenimiento."idMantenimiento" AS Mantenimiento_idMantenimiento,
     Mantenimiento."descripcion" AS Mantenimiento_descripcion,
     Mantenimiento."tipoMant" AS Mantenimiento_tipoMant,
     Equipo."idEquipo" AS Equipo_idEquipo,
     Equipo."Nombre" AS Equipo_Nombre,
     CabOrden."idOrden" AS CabOrden_idOrden,
     CabOrden."fech_reg" AS CabOrden_fech_reg,
     Empleado."idEmpleado" AS Empleado_idEmpleado,
     Empleado."apellidos" AS Empleado_apellidos,
     Empleado."nombres" AS Empleado_nombres,
     Repuesto."idRepuesto" AS Repuesto_idRepuesto,
     Repuesto."Nombre" AS Repuesto_Nombre,
     Repuesto."TipRepuesto" AS Repuesto_TipRepuesto,
     Vehiculo."idVehiculo" AS Vehiculo_idVehiculo,
     Vehiculo."marca" AS Vehiculo_marca,
     Vehiculo."modelo" AS Vehiculo_modelo,
     Vehiculo."placa" AS Vehiculo_placa,
     CabOrden."fechProg" AS CabOrden_fechProg,
     CabOrden."obs" AS CabOrden_obs,
     Cliente."idCliente" AS Cliente_idCliente,
     Cliente."nombres" AS Cliente_nombres,
     Cliente."apellidos" AS Cliente_apellidos
FROM
     "dbo"."Mantenimiento" Mantenimiento INNER JOIN "dbo"."CabOrden" CabOrden ON Mantenimiento."idMantenimiento" = CabOrden."idMantenimiento"
     INNER JOIN "dbo"."DetRepuesto" DetRepuesto ON CabOrden."idOrden" = DetRepuesto."idOrden"
     INNER JOIN "dbo"."DetEmpleado" DetEmpleado ON CabOrden."idOrden" = DetEmpleado."idOrden"
     INNER JOIN "dbo"."Vehiculo" Vehiculo ON CabOrden."idVehiculo" = Vehiculo."idVehiculo"
     INNER JOIN "dbo"."Cliente" Cliente ON CabOrden."idCliente" = Cliente."idCliente"
     INNER JOIN "dbo"."DetEquipo" DetEquipo ON CabOrden."idOrden" = DetEquipo."idOrden"
     INNER JOIN "dbo"."Empleado" Empleado ON DetEquipo."idEmpleado" = Empleado."idEmpleado"
     INNER JOIN "dbo"."Equipo" Equipo ON DetEquipo."idEquipo" = Equipo."idEquipo"
     INNER JOIN "dbo"."Repuesto" Repuesto ON DetRepuesto."idRepuesto" = Repuesto."idRepuesto"
WHERE
     Empleado."apellidos" = $P{Apellido}]]>
	</queryString>
