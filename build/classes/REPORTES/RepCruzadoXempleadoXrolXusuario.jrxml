		<![CDATA[SELECT
     Empleado."idEmpleado" AS Empleado_idEmpleado,
     Empleado."apellidos" AS Empleado_apellidos,
     Empleado."nombres" AS Empleado_nombres,
     Rol."idRol" AS Rol_idRol,
