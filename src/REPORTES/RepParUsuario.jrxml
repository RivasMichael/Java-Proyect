	<queryString>
		<![CDATA[SELECT
     usuario."idUsuario" AS usuario_idUsuario,
     usuario."idEmpleado" AS usuario_idEmpleado,
     usuario."usuario" AS usuario_usuario,
     usuario."pw" AS usuario_pw,
     usuario."fech_reg" AS usuario_fech_reg,
     usuario."fech_mod" AS usuario_fech_mod,
     usuario."estado" AS usuario_estado
FROM
     "dbo"."usuario" usuario
WHERE
     YEAR(usuario."fech_reg") =  $P{Anio}
     and MONTH(usuario."fech_reg") = $P{Mes}]]>
	</queryString>
